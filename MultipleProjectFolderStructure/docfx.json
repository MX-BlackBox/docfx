{
  "metadata": [
    {
      "src": [
        {
          "files": "src/ProjectA/**.cs",
          "exclude": [
            "**/obj/**",
            "**/bin/**"
          ],
          "src": ".."
        }
      ],
      "dest": "api/ProjectA"
    },
    {
      "src": [
        {
          "files": "src/ProjectB/**.cs",
          "exclude": [
            "**/obj/**",
            "**/bin/**"
          ],
          "src": ".."
        }
      ],
      "dest": "api/ProjectB"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/**/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ],
        "exclude": [
          "obj/**",
          "bin/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ],
        "exclude": [
          "obj/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "noLangKeyword": false
  }
}
